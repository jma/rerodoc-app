#!/bin/bash
# This hook is sourced before this virtualenv is deactivated.

unset FLASK_DEBUG
unset INVENIO_SEARCH_ELASTIC_HOSTS
unset INVENIO_SQLALCHEMY_DATABASE_URI
unset INVENIO_CELERY_BROKER_URL
unset INVENIO_CELERY_RESULT_BACKEND
unset INVENIO_CACHE_REDIS_URL
unset INVENIO_ACCOUNTS_SESSION_REDIS_URL
unset INVENIO_DB_VERSIONING
unset INVENIO_APP_ENABLE_SECURE_HEADERS
unset MAGICK_HOME